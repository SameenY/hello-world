<script type="text/javascript">
    function openWindow(){
        var mypage = comfire("是否打开新窗口？");
        if(mypage==true){
            var newpage = prompt("通过输入框，确定打开网址","http://www.imooc.com");
            if(newpage!=null){
                window.open(newpage,"_blank","width=400px,height=500px,menubar=no,toolbar=no");
            }
            else{
                alert("再见");
            }
        }
        else{
            alert("再见");
        }
    }
</script>
<body>
    <input type="button" value="新窗口打开网站" class="newpage">
</body>
